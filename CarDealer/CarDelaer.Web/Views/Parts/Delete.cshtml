@model PartListingModel
@{
    ViewData["Title"] = "Delete part";
}
<h2>@ViewData["Title"]</h2>
<div class="row">
    <div class="col-md-4">
        <hr />
        <div class="form-group">
            <label asp-for="Name"></label>
            <input asp-for="Name" disabled class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Price"></label>
            <input asp-for="Price" disabled class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Quantity"></label>
            <input asp-for="Quantity" disabled class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="SupplierName"></label>
            <input asp-for="SupplierName" disabled class="form-control" />
        </div>
        <a href="/parts/all" class="btn btn-primary">Back</a>
        <a href="/parts/ConfirmDelete?id=@Model.Id" class="btn btn-danger">Confirm Delete</a>
    </div>
</div>
