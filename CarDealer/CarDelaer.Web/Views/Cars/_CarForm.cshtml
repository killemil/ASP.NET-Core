@model CarFormModel

<form asp-route-returnUrl="@ViewData["ReturnUrl"]" method="post">
    <hr />
    <div class="form-group">
        <label asp-for="Make"></label>
        <input asp-for="Make" class="form-control" />
        <span asp-validation-for="Make" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Model"></label>
        <input asp-for="Model" class="form-control" />
        <span asp-validation-for="Model" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="TravelledDistance"></label>
        <input asp-for="TravelledDistance" class="form-control" />
        <span asp-validation-for="TravelledDistance" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="SelectedParts"></label>
        <select asp-for="SelectedParts" asp-items="Model.Parts" class="form-control car-multiple-select" multiple></select>
        <span asp-validation-for="SelectedParts" class="text-danger"></span>
    </div>

    <a class="btn btn-primary" href="/cars/all">Back</a>
    <button type="submit" class="btn btn-success">Save Car</button>
</form>


